add_primitive ("bye", agent bye)
add_primitive ("words", agent words)
add_primitive (".s", agent dot_s)
add_primitive (".", agent dot)
add_primitive (".f", agent dot_f)
add_primitive ("f.", agent fdot_defensive)

add_primitive ("sp@", agent p_sp_fetch)
add_primitive ("fp@", agent p_fp_fetch)
add_primitive ("rp@", agent p_rp_fetch)
add_primitive ("sp!", agent p_sp_store)
add_primitive ("fp!", agent p_fp_store)
add_primitive ("rp!", agent p_rp_store)
    -- Immediate primitives
add_immediate ("[']", agent imm_tick)

